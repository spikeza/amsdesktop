<Window x:Name="wLogin" x:Class="AMSDesktop.UI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AMSDesktop.UI"
        mc:Ignorable="d"
        Title="Log In" Height="228" Width="330">
    <Grid>
        <Label x:Name="lblUsername" Content="Username" HorizontalAlignment="Left" Margin="46,33,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="46,80,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.237,0.5"/>
        <TextBox x:Name="tbxUsername" HorizontalAlignment="Left" Height="23" Margin="149,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="pwbPassword" HorizontalAlignment="Left" Margin="149,80,0,0" VerticalAlignment="Top" Width="120" Height="26"/>
        <Button x:Name="btnSubmit" Content="Log In" HorizontalAlignment="Left" Margin="109,132,0,0" VerticalAlignment="Top" Width="75" Click="btnSubmit_Click"/>

    </Grid>
</Window>
