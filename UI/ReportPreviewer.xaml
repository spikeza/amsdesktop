<Window x:Class="AMSDesktop.UI.ReportPreviewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rdlcreport="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        xmlns:local="clr-namespace:AMSDesktop.UI" 
        mc:Ignorable="d"
        Title="ReportPreviewer" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <WindowsFormsHost Name="wfhMain" Grid.Row="0">
            <rdlcreport:ReportViewer x:Name="rvMain" />
        </WindowsFormsHost>
    </Grid>
</Window>
